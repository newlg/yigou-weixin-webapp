.hot{
  width: 100%;
}
.hotBody{

  display: flex;
  flex-direction: column;
  height: 100%;

}
.hotBody image{
  width: 100%;
  height: 336rpx;
  border: 1px solid #f6f6f6;

}
.hotBody .Cnavigator {
  padding-top: 15rpx;
}
.mesOther{
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  font-size: 24rpx;
  color: #808080;
  padding-top: 30rpx;
  background: #f8f8f8;

}
.mesOther text{
  padding: 10rpx 100rpx;

}
