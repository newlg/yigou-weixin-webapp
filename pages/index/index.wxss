/**index.wxss**/
.container{
  /*background: #333;*/
  width: 100%;
}
.header{
  width: 100%;
  display: flex;
  height: 58rpx;
  background-color: #fafdfd;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx 0rpx;
  position: relative;
  left: 0rpx;
  top:0rpx;
}

.header image{
  width: 154rpx;
  height: 56rpx;
  padding-left: 20rpx;
}
.header .address{
  padding-right: 20rpx;
  color: green;
}

.header .address image{
  width: 30rpx;
  height: 36rpx;
}
.header .address em{
  margin-left: 10rpx;
}
.mySwiper{
  width: 100%;

}
.slide-image{
  width: 100%;
  height: 100%;
  display: block;
}
.homeNav{
  display: flex;
  width: 100%;
  height: 186rpx;
  justify-content: space-between;


}
.homeNav.Cnavigator{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  /*padding-left: 20rpx;*/

}
.homeTitle {
  padding: 20rpx 30rpx;
}
.homeNav image{
  width: 96rpx;
  height: 96rpx;
  display: flex;
  justify-content: center;
  /*padding: 10rpx 30rpx;*/

}
.homeNav text{
  font-size: 24rpx;
  width: 25%;
  color: #9b9b9b;
}
.newKind{
  width: 100%;
}
.text{
  width:100%;
  height:44rpx;
  line-height:44rpx;
  background: #f6f6f6;
  color: #008842;
  font-size: 32rpx;
  padding-top: 20rpx;
}
.newKindBody{
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  height: 338rpx;
}
.Cnavigator{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  /*padding-left: 20rpx;*/
}
.newKindBody .newKindImage{
  width: 100%;
  height: 100%;
  border:1px solid #f6f6f6;

}
.newKindBody .newKindImage image{
  width: 100%;
  height: 100%;

}
.cost{
  width: 100%;
}
.costBody{
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  height: 336rpx;
}
.costBody .costImage{
  width: 100%;
  height: 100%;
  border:1px solid #f6f6f6;
}

.costBody .costImage image{
  width: 100%;
  height: 100%;
}
.hot{
  width: 100%;
}
.hotBody{

  display: flex;
  flex-direction: column;
  height: 100%;


}
.hotBody image{
  width: 100%;
  height: 336rpx;
  border: 1px solid #f6f6f6;
}
